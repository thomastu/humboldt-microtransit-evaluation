<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE network SYSTEM "http://www.matsim.org/files/dtd/network_v2.dtd">
<network>
<nodes>
{% for node in nodes -%}
    <node id="" x="" y= ""></node>
{%- endfor %}
</nodes>
<links>
{% for link in links -%}
    <link id ="{{link.id}}" from="{{}}" to="" length="" freespeed="" capacity="" permlanes="" oneway="1" modes="">
        <attributes>
            <attribute name="origid" class="java.lang.String">{{link.id}}</attribute>
            <attribute name="type" class="java.lang.String">{{link.linkType}}</attribute>
        </attributes>
    </link>
{% endfor %}
</links>
</network>